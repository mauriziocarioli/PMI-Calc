<scenario>
  <name>PMI Table Test.scenario</name>
  <maxRuleFirings>100000</maxRuleFirings>
  <globals/>
  <fixtures>
    <fact-data>
      <type>MortgageApplication</type>
      <fieldData>
        <field-data>
          <fieldName>property</fieldName>
          <fact>
            <type>Property</type>
            <fieldData>
              <field-data>
                <name>price</name>
                <value>200000</value>
                <nature>4</nature>
              </field-data>
            </fieldData>
          </fact>
        </field-data>
        <field-data>
          <name>program</name>
          <value>FHA</value>
          <nature>4</nature>
        </field-data>
        <field-data>
          <name>lengthOfLoanYears</name>
          <value>15</value>
          <nature>4</nature>
        </field-data>
        <field-data>
          <name>baseLoanAmount</name>
          <value>160000</value>
          <nature>4</nature>
        </field-data>
        <field-data>
          <fieldName>applicant</fieldName>
          <fact>
            <type>Applicant</type>
            <fieldData>
              <field-data>
                <name>creditScore</name>
                <value>700</value>
                <nature>4</nature>
              </field-data>
            </fieldData>
          </fact>
        </field-data>
      </fieldData>
      <name>application</name>
      <isModify>false</isModify>
    </fact-data>
    <execution-trace/>
    <expect-fact>
      <fieldValues>
        <expect-field>
          <fieldName>ltv</fieldName>
          <expected>0.8</expected>
          <operator>==</operator>
          <nature>1</nature>
        </expect-field>
        <expect-field>
          <fieldName>annualMIP</fieldName>
          <expected>45</expected>
          <operator>==</operator>
          <nature>1</nature>
        </expect-field>
      </fieldValues>
      <name>application</name>
      <anonymous>false</anonymous>
    </expect-fact>
    <expect-rule>
      <ruleName>Set LTV</ruleName>
      <expectedFire>true</expectedFire>
    </expect-rule>
    <expect-rule>
      <ruleName>Row 1 PMI Table</ruleName>
      <expectedFire>true</expectedFire>
    </expect-rule>
  </fixtures>
  <rules/>
  <inclusive>false</inclusive>
  <packageName>com.redhat.bpms.example.pmi</packageName>
  <imports>
    <imports/>
  </imports>
  <ksessions>
    <string>pmi-stateless-session</string>
  </ksessions>
</scenario>