package com.redhat.bpms.example.pmi;

import java.lang.Number;

rule "Set LTV"
	@governance(defined)
	dialect "mvel"
	ruleflow-group "initialization"
	when
		mortgage : MortgageApplication( property : property, value : property.price, loan : baseLoanAmount)
	then
		mortgage.setLtv( loan/value );
		System.out.println(">>>>> firing "+drools.getRule().getName());
end
